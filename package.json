{
  "name": "schnoin",
  "version": "0.0.1",
  "description": "Multiplayer card game \"Schnoin\"",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-client && npm run build-server",
    "build-client": "parcel build src/client/index.html",
    "build-server": "tsc --build src/server/tsconfig.json",
    "serve": "ts-node-script src/server/server.ts",
    "start": "node target/server/server.js"
  },
  "author": "Philipp Mitterer",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/parcel-bundler": "^1.12.1",
    "@types/react": "^16.9.54",
    "@types/react-dom": "^16.9.9",
    "@types/uuid": "^8.3.0",
    "modern-css-reset": "^1.1.1",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.27.0",
    "ts-node": "^9.0.0",
    "typescript": "~4.0.5"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ]
}
